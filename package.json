{
  "name": "minimist-expand",
  "description": "Expand minimist args with expand-object.",
  "version": "0.4.1",
  "homepage": "https://github.com/jonschlinkert/minimist-expand",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/minimist-expand",
  "bugs": {
    "url": "https://github.com/jonschlinkert/minimist-expand/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
